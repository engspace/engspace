{
    "user": {
        "default": {
            "permissions": []
        },
        "user": {
            "permissions": [
                "user.read",
                "project.read",
                "member.read",
                "document.read",
                "document.revise"
            ]
        },
        "manager": {
            "inherits": "user",
            "permissions": [
                "project.create",
                "project.update",
                "project.delete",
                "member.create",
                "member.update",
                "member.delete"
            ]
        },
        "admin": {
            "inherits": "manager",
            "permissions": ["user.create", "user.update", "user.delete"]
        }
    },
    "project": {
        "default": {
            "permissions": []
        },
        "designer": {
            "permissions": []
        },
        "leader": {
            "permissions": ["project.update", "member.create", "member.update", "member.delete"]
        }
    }
}
